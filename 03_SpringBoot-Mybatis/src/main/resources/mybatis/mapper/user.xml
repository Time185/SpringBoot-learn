<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="time.SpringBootMybatis.mapper.UserXml">
    <select id="getUser" resultType="time.SpringBootMybatis.entity.User">
        select * from user where uid = #{uid}
  </select>
</mapper>